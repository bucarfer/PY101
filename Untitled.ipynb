   "source": [
    "# Simple function to check if a number is even or odd\n",
    "def even_or_odd(number):\n",
    "    if number % 2 == 0:\n",
    "        return f\"{number} is even\"\n",
    "    else:\n",
    "        return f\"{number} is odd\"\n",
    "\n",
    "# Test the function\n",
    "print(even_or_odd(7))\n",
    "print(even_or_odd(12))\n"
   ]
